{
    "version": 2,
    "builds": [
      {
        "src": "simulator-server/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "public/**",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "simulator-server/server.js"
      },
      {
        "src": "/(.*)",
        "dest": "public/$1"
      }
    ]
  }
  